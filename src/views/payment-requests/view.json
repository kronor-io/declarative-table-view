                            "value": {
                                "type": "date",
                                "placeholder": "from",
                                "initialValue": {
                                    "section": "initialValues",
                                    "key": "dateRangeStart"
                                }
                            }
                        },
                        {
                            "type": "lessThanOrEqual",
                            "field": "createdAt",
                            "value": {
                                "type": "date",
                                "placeholder": "to",
                                "initialValue": {
                                    "section": "initialValues",
                                    "key": "dateRangeEnd"
                                }
                            }
                        }
                    ]
                },
                "group": "default",
                "aiGenerated": false
